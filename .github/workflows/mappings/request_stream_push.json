{
  "request": {
    "method": "POST",
    "url": "/XProtectMobile/Communication",
    "bodyPatterns": [
      { "matchesXPath": "/Communication/Command/Name[.= \"RequestStream\"]" },
      { "matchesXPath": "/Communication/ConnectionId[.= \"1b35adbb-2c02-464e-a039-15942a3a0dbf\"]" },
      { "matchesXPath": "/Communication/Command/InputParams/Param[@Name=\"MethodType\"][@Value=\"Push\"]" }
    ]
  },
  "response": {
    "status": 200,
    "fixedDelayMilliseconds": 500,
    "headers": {
      "Content-Type": "text/xml"
    },
    "bodyFileName": "request_stream.xml"
  }
}



